{
  "name": "redux-cube",
  "version": "1.0.0-rc.1",
  "description": "App state manager. A set of wrappers which simplify the use of Redux and its whole ecosystem, reduce boilerplate, and provide many features (Sub App, Reducer Bundle, ...)",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/Project-WebCube/issues"
  },
  "homepage": "https://github.com/dexteryy/Project-WebCube/tree/master/packages/redux-cube",
  "repository": "https://github.com/dexteryy/Project-WebCube/tree/master/packages/redux-cube",
  "license": "MIT",
  "engines": {
    "node": ">=6.9.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "prepare": "npm run build",
    "precommit:custom": "npm run test",
    "test": "exit 0",
    "clear": "rm -r dist lib 2>/dev/null || true",
    "build": "npm run clear && npm run build:dist && npm run build:lib",
    "build:lib": "mkdir lib && cp -r src/* lib/",
    "build:dist": "babel src --out-dir dist --source-maps"
  },
  "dependencies": {
    "change-case": "^3.0.1",
    "hifetch": "^2.1.1",
    "hoist-non-react-statics": "^2.3.1",
    "lodash": "^4.17.4",
    "normalizr": "^3.2.4",
    "react": "^16.1.1",
    "react-block-ui": "^1.1.1",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.6",
    "react-redux-toastr": "^7.1.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-debounced": "^0.4.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-observable": "^0.17.0",
    "redux-persist": "^5.3.5",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "topologically-combine-reducers": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-optional-chaining": "^7.0.0-alpha.13",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-runtime": "^6.26.0",
    "fast-async": "^6.3.0"
