{
  "cleanUrls": false,
  "headers": [
    {
      "source": "**",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=31536000; includeSubDomains"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/react-router-app/**",
      "destination": "react-router-app/index.html"
    },
    {
      "source": "/react-redux-router-app/**",
      "destination": "react-redux-router-app/index.html"
    }
  ]
}
