{
  "presets": [
    "react",
    "es2015",
    "stage-1"
  ],
  "plugins": [
  ],
  "env": {
    "dev": {
      "plugins": [
        ["react-transform", {
          "transforms": [
            {
              "transform": "react-transform-catch-errors",
              "imports": ["react", "redbox-react"]
            },
            {
              "transform": "react-transform-hmr",
              "imports": ["react"],
              "locals": ["module"]
            }
          ]
        }]
      ]
    }
  }
}
