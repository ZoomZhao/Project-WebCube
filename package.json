  "scripts": {
    "empty": "node ./node_modules/gulp/bin/gulp.js clean:empty",
    "lint": "node ./node_modules/gulp/bin/gulp.js check:all",
    "test": "node ./node_modules/gulp/bin/gulp.js test:all",
    "test:quick": "node ./node_modules/gulp/bin/gulp.js test:functional",
    "test:unit": "node ./node_modules/gulp/bin/gulp.js test:unit",
    "test:cloud": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js test:functional",
    "build": "npm run start:staticserver && node ./node_modules/gulp/bin/gulp.js build",
    "build:quick": "npm run start:staticserver && node ./node_modules/gulp/bin/gulp.js build:html",
    "new": "node ./node_modules/plop/plop.js --plopfile=configs/plopfile.babel.js",
    "dev": "node ./node_modules/gulp/bin/gulp.js watch:dev",
    "testing": "node ./node_modules/gulp/bin/gulp.js watch:units",
    "building": "npm run start:staticserver && node ./node_modules/gulp/bin/gulp.js watch:build",
    "start:staticserver": "nohup node ./node_modules/gulp/bin/gulp.js start:staticserver > configs/webserver.log 2>&1 &",
    "stop:staticserver": "node ./node_modules/gulp/bin/gulp.js stop:staticserver",
    "deploy:staticweb": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js deploy:staticweb",
    "redeploy:staticweb": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js redeploy:staticweb"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build"
    },
    "commitizen": {
