{
  "name": "webcube",
  "description": "webcube is a batch of continuously updated base code and configurations for modern static/isomorphic web app",
  "author": "dexteryy <dexter.yy@gmail.com>",
  "contributors": [],
  "bugs": {
    "url": "https://github.com/dexteryy/webcube/issues"
  },
  "homepage": "https://github.com/dexteryy/webcube",
  "repository": "https://github.com/dexteryy/webcube",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run lint && npm run build",
    "precommit": "npm run lint && npm run test",
    "update": "rm -rf ./node_modules && npm i",
    "upgrade": "rm -rf ./node_modules package-lock.json && npm i",
    "clear": "rm -rf ./configs ./tools ./utils",
    "build": "npm run build:configs && npm run build:tools && npm run build:utils",
    "build:configs": "babel src/configs --out-dir ./configs/ --source-maps && cp src/configs/*.json ./configs/ && cp src/configs/*.html ./configs/",
    "build:tools": "babel src/tools --out-dir ./tools/ --source-maps",
    "build:utils": "babel src/utils --out-dir ./utils/ --source-maps",
    "test": "npm run build",
    "lint": "eslint --fix --color --ext .js --ext .jsx",
    "lint:error": "npm run lint -- --quiet"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=6"
  },
