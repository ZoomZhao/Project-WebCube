  "scripts": {
    "empty": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js clean:empty",
    "lint": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js check:all",
    "test": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js test:all",
    "test:quick": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js test:functional",
    "test:unit": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js test:unit",
    "test:cloud": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js test:functional",
    "build": "npm run start:staticserver && node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js build",
    "build:quick": "npm run start:staticserver && node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js build:html",
    "new": "node ./node_modules/plop/plop.js --plopfile=internals/configs/plopfile.babel.js",
    "dev": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js watch:dev",
    "start:staticserver": "nohup node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js start:staticserver > internals/configs/webserver.log 2>&1 &",
    "stop:staticserver": "node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js stop:staticserver",
    "deploy:staticweb": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js deploy:staticweb",
    "redeploy:staticweb": "NODE_ENV=cloud node ./node_modules/gulp/bin/gulp.js --gulpfile internals/configs/gulpfile.babel.js redeploy:staticweb"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build"
    },
    "commitizen": {
